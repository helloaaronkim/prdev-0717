  <packaging>mule-application</packaging>
  <name>check-in-papi-template</name>
  <description>A mule application template uploaded using Exchange Maven Facade v3</description>
  <properties>
    <!-- in this WT we don't yet use a parent POM: allude to this by completely configuring properties here: -->
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    <!-- students: remove comment and replace ${app.runtime} with the version of the Mule runtime (such as 4.2.2-hf1) used in class: -->
    <!-- <app.runtime>${app.runtime}</app.runtime> -->
    <!-- students: remove comment and replace ${mule.maven.plugin.version} with the version of the Mule Maven plugin (such as 3.3.6) used in class: -->
    <!-- <mule.maven.plugin.version>${mule.maven.plugin.version}</mule.maven.plugin.version> -->
    <!-- students: remove comment and replace ${http.connector.version} with the version of the HTTP connector (such as 1.5.14) used in class: -->
    <!-- <http.connector.version>${http.connector.version}</http.connector.version> -->
    <!-- students: remove comment and replace ${apikit.version} with the version of APIkit (such as 1.3.9) used in class: -->
    <!-- <apikit.version>${apikit.version}</apikit.version> -->
    <!-- raw info for the API specification of the API exposed by this API implementation -->
    <!-- expressed through properties defined in root parent POM (BOM) -->
    <!-- students: replace ${check-in-papi.groupId} with the organization ID (such as a63e6d25-8aaf-4512-b36d-d91b90a55c4a) of your Anypoint Platform organization: -->
    <api.groupId>${check-in-papi.groupId}</api.groupId>
    <api.artifactId>${check-in-papi.artifactId}</api.artifactId>
    <!-- students: replace ${check-in-papi.version} with the version (such as 1.0.0) of the Check-In PAPI API spec in your Anypoint Platform organization: -->
    <api.version>${check-in-papi.version}</api.version>
    <tags>check-in</tags>
    <categories>[{"key":"Tier","value":"Process Tier"}]</categories>
  </properties>
  <!-- in this WT we don't yet use a parent POM: allude to this by completely configuring the Mule Maven plugin here: -->
  <build>
    <plugins>
      <plugin>
        <groupId>org.mule.tools.maven</groupId>
        <artifactId>mule-maven-plugin</artifactId>
        <version>${mule.maven.plugin.version}</version>
        <extensions>true</extensions>
        <configuration>
          <classifier>mule-application-template</classifier>
        </configuration>
      </plugin>
      <!-- students: in this WT we don't yet use MUnit: remove the entire <plugin /> element: -->
      <plugin>
        <groupId>com.mulesoft.munit.tools</groupId>
        <artifactId>munit-maven-plugin</artifactId>
      </plugin>
    </plugins>
  </build>
  <!-- in this WT we don't yet use a parent POM: allude to this by specifying the version of each dependency: -->
  <dependencies>
    <!-- API specification of the API exposed by this API implementation -->
    <!-- expressed through properties defined above -->
    <dependency>
      <groupId>${api.groupId}</groupId>
      <artifactId>${api.artifactId}</artifactId>
      <version>${api.version}</version>
      <classifier>oas</classifier>
      <type>zip</type>
    </dependency>
    <!-- limitation in Mule Maven tooling: must define before apikit! -->
    <dependency>
      <groupId>org.mule.connectors</groupId>
      <artifactId>mule-http-connector</artifactId>
      <version>${http.connector.version}</version>
      <classifier>mule-plugin</classifier>
    </dependency>
    <dependency>
      <groupId>org.mule.modules</groupId>
      <artifactId>mule-apikit-module</artifactId>
      <version>${apikit.version}</version>
      <classifier>mule-plugin</classifier>
    </dependency>
    <!-- standard dependencies for all Mule apps - start -->
    <!-- limitation in Mule Maven tooling: must be defined before apikit! -->
    <!--
    <dependency>
